{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "SoundLab",
	"version": "0.1.0",
	"identifier": "com.prodbyeagle.soundlab",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "SoundLab",
				"width": 800,
				"height": 600,
				"decorations": false,
				"minHeight": 800,
				"minWidth": 480
			}
		]
	},
	"bundle": {
		"active": true,
		"targets": "msi",
		"icon": ["icons/icon.png", "icons/icon.ico"]
	}
}
